<% include ../partials/header.ejs %>

<div class="form-heading">Sign In</div>
<form class="form-signup" action="/sign-in" method="POST">
  <div class="form-item form-email">
    <span>Email</span>
    <input type="email" name="email" value=<%= autofill.email %>>
  </div>
  <div class="form-item form-password">
    <span>Password</span>
    <input type="password" name="password" value=<%= autofill.password %>>
  </div>
  <input class="btn btn-form-submit" type="submit"></input>
</form>

<% include ../partials/footer.ejs %>