<% include ../partials/header.ejs %>

<form action="/sign-up" method="POST">
  <ul>
    <li>
      <p>Email</p>
      <input type="email" name="email" placeholder="user@email.com">
    </li>
    <li>
      <p>Name</p>
      <input type="text" name="name" placeholder="Alex Baldwin">
    </li>
    <li>
      <p>Password</p>
      <input type="password" name="password" placeholder="********" value=<%= autofill.password %>>
    </li>
    <li>
      <p></p>
      <input type="submit">
    </li>
  </ul>
</form>

<% include ../partials/footer.ejs %>